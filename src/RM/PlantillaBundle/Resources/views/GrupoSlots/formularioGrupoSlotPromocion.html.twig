{{ form_start(form) }}

{% include '@RMPlantilla/GrupoSlotsModelo/form_promocion.html.twig' with {'form' : form} %}

<table cellpadding="0" cellspacing="0" border="0">
    <tbody>
    <tr>
        <td width="100">{{ form_widget(form.submit, { 'attr': {'class': 'btn btn-primary'}, 'label': 'boton.guardar'|trans|title }) }}</td>
        <td width="100">
            <button class="btn btn-warning" onclick="window.location.href='{{ path('rm_comunicacion.comunicacion.editar_plantilla', {'idComunicacion': comunicacion.idComunicacion} ) }}'; return false;">{{ "boton.volver" |trans }}</button>
        </td>
    </tr>
    </tbody>
</table>
<input type="hidden" name="idComunicacion" value="{{ comunicacion.idComunicacion }}"/>
{{ form_rest(form) }}
{{ form_end(form) }}